{
  "name": "ngx-trend",
  "version": "3.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rimraf dist && ts-node build.ts",
    "publish": "cd dist/packages-dist && npm publish",
    "bundlesize": "bundlesize",
    "test": "ng test --code-coverage --single-run",
    "lint": "ng lint --type-check",
    "lint:fix": "ng lint --type-check --fix",
    "e2e": "ng e2e",
    "cleanup": "rimraf build dist waste deploy",
    "ghpages": "ng build --prod --aot --build-optimizer --no-progress --baseHref='/ngx-trend/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ctrl/ngx-github-buttons": "1.0.2",
    "core-js": "^2.5.3",
    "ng2-nouislider": "1.7.6",
    "nouislider": "^10.1.0",
    "rxjs": "^5.5.5",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.0.58",
    "bundlesize": "^0.15.3",
    "codelyzer": "^4.0.2",
    "copy": "^0.3.1",
    "fs-extra": "^5.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.2.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "2.5.x"
  },
  "bundlesize": [
    {
      "path": "./dist/packages-dist/bundles/*.js",
      "maxSize": "5 kB"
    }
  ]
}
