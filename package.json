    "@angular/core": "7.2.3",
    "@angular/forms": "7.2.3",
    "@angular/material": "7.3.0",
    "@angular/platform-browser": "7.2.3",
    "@angular/platform-browser-dynamic": "7.2.3",
    "@angular/router": "7.2.3",
    "@ctrl/ngx-github-buttons": "2.1.0",
    "core-js": "2.6.3",
    "hammerjs": "2.0.8",
    "rxjs": "6.4.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.0",
